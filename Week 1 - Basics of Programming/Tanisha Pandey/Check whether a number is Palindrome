#include <iostream>
using namespace std;

int reverseNum (int num)
{
    while(num!=0)
    {
      int digit = num % 10;
      rev = (rev * 10) + digit;
      num = num / 10;
    }  
    
    return num;
    
}

int main()
{
     int n, num, digit, rev = 0;

     cin >> num;
     n = num;
     rev = reverseNum(num);

     if (n == rev)
         cout << " The number is a palindrome.";
     else
         cout << " The number is not a palindrome.";

    return 0;
}
